{
  "name": "E2E Non-AI Hybrid",
  "description": "Data steps + tool step + subflow composition",
  "steps": [
    {
      "step_id": "trigger",
      "step_type": "trigger",
      "parameters": {
        "kind": "webhook"
      }
    },
    {
      "step_id": "data_in",
      "step_type": "data_input",
      "dependencies": [
        "trigger"
      ],
      "config": {
        "input_type": "static",
        "data": {
          "a": 5,
          "b": 7
        }
      }
    },
    {
      "step_id": "merge",
      "step_type": "data_merge",
      "dependencies": [
        "data_in",
        "trigger"
      ]
    },
    {
      "step_id": "add",
      "step_type": "tool_execution",
      "dependencies": [
        "merge"
      ],
      "input_mapping": {
        "merge": "merge.result"
      },
      "config": {
        "tool_name": "add_numbers",
        "param_mapping": {
          "a": "step_merge.result.data.a",
          "b": "step_merge.result.data.b"
        },
        "static_params": {}
      }
    },
    {
      "step_id": "subflow",
      "step_type": "subflow",
      "dependencies": [
        "add"
      ],
      "config": {
        "workflow_id": "__PLACEHOLDER__",
        "input_mapping": {
          "sum": "add.result"
        },
        "output_mapping": {}
      }
    }
  ]
}