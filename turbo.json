{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "globalEnv": [
    "AUTH_SECRET",
    "AWS_REGION",
    "AWS_BUCKET_NAME",
    "AWS_AKID",
    "AWS_SAK",
    "NODE_ENV",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "FUSIONAUTH_CLIENT_ID",
    "FUSIONAUTH_CLIENT_SECRET",
    "FUSIONAUTH_ISSUER",
    "FUSIONAUTH_URL",
    "FUSIONAUTH_API_KEY",
    "FUSIONAUTH_TENTANT_ID",
    "FUSIONAUTH_APPLICATION_ID",
    "NEXTAUTH_URL",
    "NEXTAUTH_URL_INTERNAL",
    "NEXT_PUBLIC_BACKEND_URL",
    "NEXT_PUBLIC_MODELS_API_URL",
    "NEXT_PUBLIC_CONTRACTS_API_URL",
    "ELEVAITE_HOMEPAGE",
    "ORG_ID",
    "RBAC_BACKEND_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "cache": false
    },
    "type-check": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "docker": {
      "cache": false
    },
    "storybook": {}
  }
}
