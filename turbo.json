{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": [
    "AUTH_SECRET",
    "AWS_REGION",
    "AWS_BUCKET_NAME",
    "AWS_AKID",
    "AWS_SAK",
    "NODE_ENV",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "FUSIONAUTH_CLIENT_ID",
    "FUSIONAUTH_CLIENT_SECRET",
    "FUSIONAUTH_ISSUER",
    "FUSIONAUTH_URL",
    "FUSIONAUTH_API_KEY",
    "FUSIONAUTH_TENTANT_ID",
    "FUSIONAUTH_APPLICATION_ID",
    "MODELS_PASSWORD",
    "MODELS_USERNAME",
    "NEXTAUTH_URL",
    "NEXTAUTH_URL_INTERNAL",
    "NEXT_PUBLIC_BACKEND_URL",
    "NEXT_PUBLIC_MODELS_API_URL",
    "NEXT_PUBLIC_CONTRACTS_API_URL",
    "NEXT_PUBLIC_CONTRACTS_IOPEX_API_URL",
    "ELEVAITE_HOMEPAGE",
    "ORG_ID",
    "RBAC_BACKEND_URL",
    "NODE_TLS_REJECT_UNAUTHORIZED",
    "NEXT_PUBLIC_ARLO_BACKEND_URL",
    "NEXT_PUBLIC_AUTH_API_URL",
    "NEXT_PUBLIC_AUTH_TENANT_ID",
    "NEXT_PUBLIC_MFA_METHODS_ENABLED",
    "NEXT_PUBLIC_MFA_ALLOW_DISABLE_ALL",
    "AUTH_TENANT_ID",
    "AUTH_API_URL",
    "BACKEND_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "cache": false
    },
    "type-check": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "docker": {
      "cache": false
    },
    "storybook": {}
  }
}
